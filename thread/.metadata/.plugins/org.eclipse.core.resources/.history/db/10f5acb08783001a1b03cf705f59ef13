package sync;

public class ThreadSyncTest01 {
	public static void main(String[] args) {
		//모든 쓰레드가 공유해서 사용할 객체
		Toilet toilet = new Toilet();
		
		//공유객체를 사용하는 쓰레드 객체를 생성
		User user1 = new User("장동건",toilet);
		User user2 = new User("장동건",toilet);
		User user3 = new User("장동건",toilet);
		User user4 = new User("장동건",toilet);
		User user5 = new User("장동건",toilet);
		User user6 = new User("장동건",toilet);
		User user7 = new User("장동건",toilet);
		
	}

}
